    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "check": "tsc --noEmit",
    "create-chart": "node ./scripts/GenerateComponent/index.js",
    "help": "max help",
    "static-dev": "nodemon ./scripts/StaticFileDebug/index.js"
  },
